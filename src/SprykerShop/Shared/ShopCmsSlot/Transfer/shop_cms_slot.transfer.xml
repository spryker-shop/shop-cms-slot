<?xml version="1.0"?>
<transfers
    xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd"
>

    <transfer name="CmsSlotContentResponse">
        <property name="content" type="string"/>
    </transfer>

    <transfer name="CmsSlotContentRequest">
        <property name="cmsSlotKey" type="string"/>
        <property name="params" type="array"/>
    </transfer>

    <transfer name="CmsSlotContext">
        <property name="cmsSlotKey" type="string"/>
        <property name="providedData" type="array"/>
        <property name="requiredKeys" type="string[]"/>
        <property name="autoFilledKeys" type="string[]"/>
    </transfer>

    <transfer name="CmsSlotExternalData">
        <property name="values" type="string[]" associative="true"/>
    </transfer>

    <transfer name="CmsSlotStorage">
    </transfer>

</transfers>
