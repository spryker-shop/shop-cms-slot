<?xml version="1.0"?>
<transfers
    xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd"
>

    <transfer name="CmsSlotContentResponse">
        <property name="content" type="string"/>
    </transfer>

    <transfer name="CmsSlotContentRequest">
        <property name="cmsSlotKey" type="string"/>
        <property name="cmsSlotParams" type="CmsSlotParams"/>
        <property name="cmsSlotTemplatePath" type="string"/>
    </transfer>

    <transfer name="CmsSlotContext">
        <property name="cmsSlotKey" type="string"/>
        <property name="providedData" type="array" singular="providedData"/>
        <property name="requiredKeys" type="string[]" singular="requiredKeys"/>
        <property name="autoFilledKeys" type="string[]" singular="autoFilledKeys"/>
        <property name="cmsSlotTemplatePath" type="string"/>
    </transfer>

    <transfer name="CmsSlotExternalData">
        <property name="values" type="string[]" associative="true" singular="values"/>
    </transfer>

    <transfer name="CmsSlotParams">
    </transfer>

    <transfer name="CmsSlotStorage">
        <property name="isActive" type="bool"/>
        <property name="contentProviderType" type="string"/>
    </transfer>

</transfers>
