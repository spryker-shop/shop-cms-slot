{
  "name": "spryker-shop/shop-cms-slot",
  "description": "ShopCmsSlot module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker-shop/shop-application": "^1.6.0",
    "spryker-shop/shop-cms-slot-extension": "^1.0.0",
    "spryker/cms-slot": "^1.1.0",
    "spryker/error-handler": "^2.3.0",
    "spryker/kernel": "^3.37.0",
    "spryker/twig": "^3.7.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerShopTest\\": "tests/SprykerShopTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
